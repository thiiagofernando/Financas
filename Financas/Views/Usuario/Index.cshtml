@model IList<Financas.Entidades.Usuario>

@Html.ActionLink("Novo Usuario", "Form")
<table class="table table-hover">
    <thead>
    <th>ID</th>
    <th>Nome</th>
    <th>E-mail</th>
    </thead>
    <tbody>
    @foreach (var usuario in Model)
    {
        <tr>
            <td>@usuario.Id</td>
            <td>@usuario.Nome</td>
            <td>@usuario.Email</td>
        </tr>
  
    }
    </tbody>
</table>